php_value magic_quotes_gpc Off

<FilesMatch "^(searchUser|searchUserCAS|searchUserTrusted)$">
    SetHandler application/x-httpd-php
</FilesMatch>

<FilesMatch "\.inc\.php$">
    Order allow,deny
</FilesMatch>

<FilesMatch "^(searchUserCAS)$">
    Order allow,deny
    Allow from all
    AuthType CAS
    require valid-user
</FilesMatch>

