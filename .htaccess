php_value magic_quotes_gpc Off

<FilesMatch "^(searchUser|searchUserCAS|searchUserTrusted)$">
    SetHandler application/x-httpd-php
</FilesMatch>

<FilesMatch "\.html\.js$">
    SetHandler application/x-httpd-php
</FilesMatch>

<FilesMatch "\.inc\.php$">
    Order allow,deny
</FilesMatch>

<FilesMatch "^(search|userGroupsId|userGroupsAndRoles|getSubGroups|allGroups|groupUsers)$">
    SetHandler application/x-httpd-php
</FilesMatch>
