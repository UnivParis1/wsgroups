php_value magic_quotes_gpc Off

RewriteEngine on
RewriteRule ^(\w+)$ $1.php [NC,L]
RewriteRule ^(.*\.html\.js)$ $1.php [NC,L]

<FilesMatch "\.html\.js$">
    SetHandler application/x-httpd-php
</FilesMatch>

<FilesMatch "\.inc\.php$">
    Order allow,deny
</FilesMatch>

<FilesMatch "^(annuaire.html)$">
    Order allow,deny
    Allow from all
    AuthType CAS
    require valid-user
</FilesMatch>
